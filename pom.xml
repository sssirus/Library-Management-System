		<dependency>
			<groupId>org.ansj</groupId>
			<artifactId>ansj_lucene6_plug</artifactId>
			<version>5.1.1.1</version>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-thymeleaf</artifactId>

